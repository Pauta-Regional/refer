<Page xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      WindowTitle="Hollow Cube"
      Title="Hollow Cube">
    <Page.Resources>
        <Style TargetType="Polygon">
            <Setter Property="StrokeStartLineCap" Value="Round" />
            <Setter Property="StrokeEndLineCap" Value="Round" />
            <Setter Property="StrokeLineJoin" Value="Round" />
        </Style>
    </Page.Resources>

    <Canvas>
        <Polygon Points="96 96, 192 96, 192 192, 96 192"
                 Stroke="Black" />

        <Polygon Points="144 144, 240 144, 240 240, 144, 240"
                 Stroke="Black" />

        <Polygon Points="96 96, 192 96, 240 144, 144 144"
                 Stroke="Black" />

        <Polygon Points="96 96, 144 144, 144 240, 96 192"
                 Stroke="Black" /> 

        <Polygon Points="192 96, 240 144, 240 240, 192 192"
                 Stroke="Black" />
    </Canvas>
</Page>
